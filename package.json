{
  "name": "lifestylemaniacs",
  "version": "1.0.0",
  "description": "Lifestyle Blog",
  "author": "Razvan Tomegea",
  "private": true,
  "scripts": {
    "build": "npm run lint && nuxt build",
    "deploy": "npm run generate && firebase deploy",
    "dev": "cross-env NODE_ENV=dev nuxt",
    "generate": "nuxt generate",
    "heroku-postbuild": "npm run build",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .  --fix",
    "precommit": "npm run lint",
    "serve": "npm run generate && npm run serve:firebase",
    "serve:firebase": "firebase serve",
    "start": "npm run dev",
    "start:prod": "npm run build && nuxt start"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/vuetify": "0.5.5",
    "firebase": "^6.3.3",
    "lodash": "^4.17.15",
    "mammoth": "^1.4.8",
    "nuxt": "^2.0.0",
    "reading-time": "^1.2.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/eslint-module": "^0.0.1",
    "babel-eslint": "^10.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.1",
    "eslint-config-standard": ">=12.0.0",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-promise": ">=4.0.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "nodemon": "^1.18.9",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
